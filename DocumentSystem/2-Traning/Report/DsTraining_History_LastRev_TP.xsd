                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        A.EmployeeNo, D.EmployeeName, MAX(A.TrnnDate) AS MAXTrannDate, A.DOCNO, C.DOCNAME, MAX(A.REVNO) AS MAXREVNO, B.MthdNam, A.Result, A.Approve, A.Remark, A.Dept, D.Section, A.RetrainFreq
FROM            QEa_Training_History AS A LEFT OUTER JOIN
                         QEa_Training_Method AS B ON A.MthdCode = B.MthdCode LEFT OUTER JOIN
                         QEa_CourseMaster AS C ON A.DOCNO = C.DOCNO LEFT OUTER JOIN
                         QEa_Training_Person AS D ON A.EmployeeNo = D.EmployeeNo
GROUP BY A.EmployeeNo, D.EmployeeName, A.DOCNO, B.MthdNam, A.Result, A.Approve, A.Remark, A.Dept, D.Section, C.DOCNAME, A.RetrainFreq
HAVING        (A.EmployeeNo = @Param1) AND (A.Dept = @Param2)
ORDER BY A.DOCNO, MAXREVNO, A.RetrainFreq, MAXTrannDate, A.EmployeeNo</CommandText>
