                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        A.EmployeeNo, F.EmployeeName, F.Dept, F.Section, A.DOCNO, E.DOCNAME, D.AssignName, C.TrainerName, A.RetrainFreq
FROM            QEa_Training_Assign AS A LEFT OUTER JOIN
                         QEa_Training_Method AS B ON A.MthdCode = B.MthdCode LEFT OUTER JOIN
                         QEa_Training_Method_Trainer AS C ON A.Trainer = C.TrainerCode LEFT OUTER JOIN
                         QEa_Training_Method_Assign AS D ON A.Assign = D.AssignCode LEFT OUTER JOIN
                         QEa_CourseMaster AS E ON A.DOCNO = E.DOCNO LEFT OUTER JOIN
                         QEa_Training_Person AS F ON A.EmployeeNo = F.EmployeeNo
GROUP BY A.EmployeeNo, F.EmployeeName, F.Dept, F.Section, A.DOCNO, E.DOCNAME, D.AssignName, C.TrainerName, A.RetrainFreq
HAVING        (A.EmployeeNo = @Param1) AND (F.Dept = @Param2)</CommandText>
